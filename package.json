    "build:qa2": "vue-cli-service build --mode qa2",
    "build:qa3": "vue-cli-service build --mode qa3",
    "preview": "node build/index.js --preview",
    "svgo": "svgo -f src/jsvue/icons/svg --config=src/jsvue/icons/svgo.yml",
    "test:unit": "jest --clearCache && vue-cli-service test:unit",
    "test:ci": "npm run lint && npm run test:unit",
    "proto": "node build/gen_proto.js",
    "proto1": "node build/gen_proto1.js",
    "proto2": "node build/gen_proto2.js"
  },
